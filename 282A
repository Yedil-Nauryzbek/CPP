#include <iostream>
using namespace std;
int main() {
    int a,s=0;
    cin>>a;
    string arr[a];
    for(int i=0;i<a;i++) {
    cin>>arr[i];
    if(arr[i]=="X++" or arr[i]=="++X") {
        s+=1;
    }
    else if(arr[i]=="X--" or arr[i]=="--X") {
        s-=1;
    }
    }
    cout<<s;
    return 0;
}
